{
  "name": "react-starter-kit",
  "version": "1.0.0",
  "description": "Настройка окружения для React",
  "scripts": {
    "start": "webpack-dev-server --mode development --config webpack.config.dev.js",
    "dev": "cross-env NODE_ENV=dev webpack --mode development --config webpack.config.dev.js",
    "build": "webpack -p --progress --mode production --config webpack.config.build.js",
    "lint:js:fix": "eslint --fix --ext .es,.jsx ./src",
    "lint:js": "eslint --ext .es,.jsx ./src",
    "lint:css:fix": "stylefmt -r ./src/**/*.scss",
    "lint:css": "stylelint ./src/**/*.scss"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "react",
    "starterkit"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:js:fix & yarn lint:css:fix & git add .",
      "pre-push": "yarn lint:js & yarn lint:css"
    }
  },
  "author": "TogyzbaevTN",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^8.3.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-stage-2": "^6.24.1",
    "mini-css-extract-plugin": "^0.4.0",
    "postcss-flexbugs-fixes": "^3.3.0",
    "postcss-gradient-transparency-fix": "^2.1.0",
    "postcss-loader": "^2.1.4",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-modal": "^3.4.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "stylelint-config-sass-guidelines": "^5.0.0",
    "stylelint-webpack-plugin": "^0.10.4"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "0.1.19",
    "cross-env": "^5.1.4",
    "css-loader": "0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "1.1.11",
    "html-webpack-harddisk-plugin": "^0.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.0.0-rc.1",
    "node-sass": "^4.8.3",
    "path": "^0.12.7",
    "prettier": "^1.12.1",
    "sass-loader": "7.0.1",
    "serve-favicon": "~2.4.5",
    "style-loader": "0.20.3",
    "stylefmt": "^6.0.0",
    "stylelint": "^9.2.0",
    "url-loader": "1.0.1",
    "webpack": "4.5.0",
    "webpack-cli": "2.0.14",
    "webpack-dev-server": "3.1.3",
    "webpack-merge": "4.1.2"
  }
}
